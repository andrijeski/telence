{
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "deno"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "strict": true,
    "allowImportingTsExtensions": true,
    "noEmit": true, 
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowJs": false,
    "noFallthroughCasesInSwitch": true
  }
}
